[hub]
        protocol = ssh
[user]
        name = {{ full_name }}
        email = {{ git_email }}
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        attributesfile = ~/.gitattributes
        editor = vim
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[pull]
        rebase = true
[github]
        user = {{ git_user }}
